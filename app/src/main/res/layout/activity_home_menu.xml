<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/brown8"
    tools:context="com.et.fivekilled.HomeMenu">

    <android.support.design.widget.FloatingActionButton
        app:backgroundTint="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/singlePlayerButton"
        android:src="@drawable/singlet"
        android:elevation="16dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <!--<android.support.design.widget.FloatingActionButton-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:id="@+id/multiPlayerButton"-->
        <!--android:src="@drawable/multi"-->
        <!--app:backgroundTint="@android:color/darker_gray"-->
        <!--android:layout_alignTop="@+id/singlePlayerButton"-->
        <!--android:layout_alignParentEnd="true"-->
        <!--android:layout_marginEnd="87dp" />-->
    <Helpers.fonts.EtTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="55dp"
        android:textColor="@android:color/black"
        android:text="ALPHAKILL"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="79dp"
        android:id="@+id/fk" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@android:color/black"
        android:src="@drawable/help_icon"
        android:id="@+id/helpButton"
        android:elevation="16dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="51dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@android:color/holo_blue_dark"
        android:src="@drawable/lboard_icon"
        android:id="@+id/lboardButton"
        android:elevation="16dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/helpButton"
        android:layout_toEndOf="@+id/show_achievement_button" />



    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@android:color/holo_orange_dark"
        android:src="@drawable/hscore"
        android:id="@+id/high_score_button"
        android:elevation="16dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/lboardButton"
        android:layout_toEndOf="@id/lboardButton" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="@android:color/holo_blue_dark"
        android:src="@drawable/ach_ach"
        android:id="@+id/show_achievement_button"
        android:elevation="16dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/helpButton"
        android:layout_toEndOf="@+id/helpButton" />
    <com.google.android.gms.ads.AdView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/home_admob_banner"
        android:layout_alignParentTop="true"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="@string/admob_egr_banner_adunit_id"/>


</RelativeLayout>
