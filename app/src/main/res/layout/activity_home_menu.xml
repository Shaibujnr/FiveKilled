<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/brown8"
    tools:context="com.et.fivekilled.HomeMenu">

    <android.support.design.widget.FloatingActionButton
        app:backgroundTint="@android:color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/singlePlayerButton"
        android:src="@drawable/singlet"
        android:layout_marginBottom="66dp"
        android:elevation="16dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/multiPlayerButton"
        android:src="@drawable/multi"
        app:backgroundTint="@android:color/darker_gray"
        android:layout_alignTop="@+id/singlePlayerButton"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="87dp" />
    <Helpers.fonts.MenuTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5KILLED"
        android:textColor="@android:color/black"
        android:textSize="80dp"
        android:textStyle="italic"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="79dp"
        android:id="@+id/fk" />


</RelativeLayout>
